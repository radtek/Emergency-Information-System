@model IEnumerable<EmergencyInformationSystem.Models.ViewModels.Reports.StatisticsMonth.GreenPathThird>

@foreach (var item in Model)
{
    <tr>
        <td style="padding-left:80px">
            @Html.ActionLink(item.GreenPathCategoryRemarksForDisplay, "IndexGreenPath", new { item.GreenPathCategoryId, item.GreenPathCategoryRemarks, item.Time }, new { target = "_blank" })
        </td>
        <td style="padding-left:80px">
            @Html.DisplayFor(model => item.Count)
        </td>
    </tr>
}