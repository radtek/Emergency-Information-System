@model EmergencyInformationSystem.Models.ViewModels.RescueRoomInfos2.TransferRoom.TransferRoom

@{
    ViewBag.Title = "抢救室病例信息表 - 转室";
}

@*面包屑区*@
<div class="container">
    <ol class="breadcrumb">
        <li>@Html.ActionLink("主界面", "Index", "Home", null, new { @class = "btn-show-modal" })</li>
        <li>@Html.ActionLink("抢救室病例信息一览表", "Index", null, new { @class = "btn-show-modal" })</li>
        <li class="active">转室</li>
    </ol>
</div>

<div class="container">
    @*标题*@
    <div class="row RescueRoomMainTitleRow">
        <div class="h2 RescueRoomMainTitleCell">
            @ViewBag.Title
        </div>
        <div>
            <p>
                选择接诊记录。
                <br />
            </p>
        </div>
    </div>

    @*列表区*@
    @Html.Partial("TransferRoomListGhxx", Model.ListGhxx)

    <div>
        <table class="table">
            <tr>
                <td class="text-center">仅显示转室记录时间之后的项目。</td>
            </tr>
        </table>
    </div>
</div>

@section scripts{
    @Scripts.Render("~/bundles/EisNormal")
}